@charset "utf-8";
/* CSS Document */

body {
  background:#fff;
  margin: 0px auto;
  height:auto;
  font-size:15px;
  font-family:'asapregular',Arial, Helvetica, sans-serif;
  color:#393939;
  height:100%;
}

@font-face {
    font-family: 'asapbold';
    src: url('asap-bold-webfont.eot');
    src: url('asap-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('asap-bold-webfont.woff') format('woff'),
         url('asap-bold-webfont.ttf') format('truetype'),
         url('asap-bold-webfont.svg#asapbold') format('svg');
    font-weight: normal;
    font-style: normal;
}


@font-face {
    font-family: 'asapregular';
    src: url('asap-regular-webfont.eot');
    src: url('asap-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('asap-regular-webfont.woff') format('woff'),
         url('asap-regular-webfont.ttf') format('truetype'),
         url('asap-regular-webfont.svg#asapregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

.attach {
  -moz-opacity: 0;
  opacity: 0;
  filter: alpha(opacity= 0);
  width: 25px;
  position: absolute;
  left: 0px;
}


.pages{
  float: left;
  margin-left: 410px;
  padding-bottom: 30px;
  margin-bottom: 30px;
  max-width: 80%;
  padding-top: 20px;
  min-height: 600px;;

  .chat-wrap {
    padding-left: 5px;
    padding-bottom: 5px;
    margin-bottom: 10px;


    .chat {
      box-shadow: 0px 0px 10px #f9a435;
      float: left;
      margin-top: 0;
      margin-right: 5px;
    }

    .profile_detail {
      margin-left: 476px;
      /*width: 32%;*/
      width: 565px;
      padding: 20px 40px;
      background-color: #3c3832;
      box-shadow: 0px 0px 3px #3c3832;
      position: relative;
      h2 {
        color: #fbf8f8;
        font-weight: bold;
        font-size: 36px;
        margin-top: 0;
        margin-bottom: 20px;
      }
      h3 {
        margin-top: 0;
        color: #f9a435;
        font-size: 24px;
        font-weight: normal;
        font-style: italic;
        margin-bottom: 10px;
      }
      h4 {
        font-size: 18px;
        font-style: italic;
        font-weight: normal;
      }
      .web_address {
        font-size: 12px;
        font-weight: bold;
      }
      h4, .web_address {
        color: #93b345;
      }

      .web_address {
        margin-bottom: 20px;
        border-bottom: 2px solid #b0a076;
        padding-bottom: 5px;
      }

      .detail_photo {
        position: absolute;
        right: 20px;
        top: 20px;
        width: 49px;
        height: 49px;
      }
      ul.friend_requests {
        padding: 0;
        margin: 0;
        overflow: auto;
        list-style: none;

        li {
          float : left;
          width: 280px;
          margin-bottom: 30px;
          margin-right: 0px;


          a.req_link {
            display: block;
            padding: 22px 20px;
            font-size: 26px;
            text-shadow: 0px -1px 0px #374683;
            line-height: 14px;
            font-weight: bold;
            text-align: center;
            background: -webkit-gradient(linear, left top, left bottom, from(#fac98e), to(#faa435));
            background: -moz-linear-gradient(top,  #fac98e,  #faa435);
            filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fac98e', endColorstr='#faa435');
            color: #feffff;

            border-radius: 8px;
            -moz-border-radius: 8px;
            -webkit-border-radius: 8px;

            border: 1px solid #fce7bc;
          }
        }
        .fake_photo {
          height: 130px;
          width: 130px;          
        }
        .add_box {
          border: 3px solid #ffffff;
          border-radius: 10px;
          color: #fff;
          text-align: center;
          /*padding: 25px 15px;*/
          display: table-cell;
          height: 130px;
          width: 140px;
          padding: 0;
          vertical-align: middle;
          text-align: center;
          font-size: 18px;
          text-transform: capitalize;

          &.silent {
            border-width: 0;
          }
        }
        .small {
          height: 30px;
          width: 30px;            
          display: block;
          float: none;
          margin-left: 150px;
          margin-top: 50px;
        }
      }
    }
  }
}

.search-container {
  margin-left: 410px;
  margin-top: 20px;
  margin-right: 10px;
}

.header{
  background: url(<%= asset_path('fondo-cabezal.jpg') %>) repeat-x top;
  top: 0px;
  width: 100%;
  min-width: 780px;
  height: 33px;
  float: left;
  z-index: 9999;
  text-align: right;
  padding-top: 7px;
  position: fixed;
  box-shadow: 0px 2px 5px #aaa;

  a.log-out:link,a.log-out:active,a.log-out:visited{
    background:url(<%= asset_path('close.png') %>) no-repeat left;
    margin-right: 10px;
    padding-left: 22px;
    float:right;
  }

  .header-name{
    padding-right: 2px;
    float:right;
  }
}

.sidebar {
  background: url(<%= asset_path 'body_bg.png'%>);
  position:fixed;
  top: 0px;
  left: 0px;
  height:100%;
  width:400px;
  padding-left: 0px;
  float:left;
  min-height:600px;
  padding-top: 40px;
  box-shadow: -3px 0px 3px #aaa inset;
  &.full_screen {
    width: 100%;
    box-shadow: 0px 0px 0px #aaa inset;
  }
}

.sidebar, .pages, .search-container, .frame_layout {
  .bold {
    font-family:'asapbold',Arial, Helvetica, sans-serif;
  }

  textarea {
    resize: none;
  }

  // a:link {color:#666;text-decoration:none;font-weight:normal;}
  // a:active {color:#666;text-decoration:none;font-weight:normal;}
  // a:visited {color:#666;text-decoration:none;font-weight:normal;}
  // a:hover {color:#000;text-decoration:none;font-weight:normal;}

  .clear {
    clear: both;
  }

  input[type=search] {-webkit-appearance: none;}
  input[type="search"]::-webkit-search-decoration, 
  input[type="search"]::-webkit-search-cancel-button {display: none;}
  input:focus{outline: none; color:#000;}
  textarea:focus{outline: none; color:#000;}

